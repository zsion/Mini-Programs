@import "/common/navtab.wxss";
.zgss-head{
  width:100%;
  height:auto;
  background: #00BEEE;
}
.add-fixed{
  position: fixed;
  top:0;
  left:0;
  z-index: 3;
}
.search-wrap {display:flex;justify-content:space-around;align-items:center;width:90%;height:100rpx;margin:auto;}
.search-wrap input{
  width:82%;
  padding:15rpx;
  background: #F2F3F5;
  border-radius:35rpx;
  font-size: 28rpx;
}
.search-wrap view{
  width:70rpx;
  height:70rpx;
  padding:0;
  background: transparent;
}
.search-wrap view::after{ border: none; }
.search-wrap view image{width:100%;}
.explain{
  width:100%;
  height:50rpx;
  line-height: 50rpx;
  background:#FFF6F1;
  text-align: center;
  font-size: 24rpx;
  color:#A99090;
}
.list-tab{
  width:100%;
  height:70rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background: #fff;
}
.list-tab view{
  padding:0 15rpx 0 15rpx;
  height:70rpx;
  line-height: 70rpx;
  text-align: center;
  font-weight:bold;
}
.zgss-body{
  padding-top: 220rpx;
}
.for-list{
  width:94%;
  background: #fff;
  margin-top: 30rpx;
  padding:15rpx 3% 15rpx 3%;
}
.for-list view:first-child{
  font-size: 30rpx;
  color:#31363B;
}
.for-list view:not(:first-child){
  font-size: 26rpx;
  color:#31363B;
  line-height: 50rpx;
}
.for-list view:last-child{
  display: flex;
}
.for-list view:last-child navigator{
  margin-left: 15rpx;
  color:#00B1E0;
  text-decoration: underline;
}





