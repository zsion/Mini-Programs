.searchzgss-head{
  width:100%;
  height:auto;
  background: #fff;
}

.add-fixed{
  position: fixed;
  top:0;
  left:0;
  z-index: 3;
}
.add-head{
  height: 70rpx;
  border-bottom: 1px solid #E1EDEE;
}
.add-head text:first-child{
  background: #00B1E0;
  color:#00B1E0;
  line-height: 70rpx;
  margin:0 10rpx 0 25rpx;
}
.add-head text:last-child{
  line-height: 70rpx;
  font-size: 30rpx;
  color:#31363B;
}
.list-tab{
  width:100%;
  height:70rpx;
  display: flex;
  justify-content: space-between;
  background: #fff;
}
.list-tab view{
  width:100%;
  height:70rpx;
  line-height: 70rpx;
  text-align: center;
}
.list-tab view text{
  font-size: 30rpx;
  color: #5C5E5F;
  padding: 9rpx;
}
.list-tab view.high-light text{
  color: #0F0F0F;
  border-bottom: 5rpx solid #00BEEE;
}
.searchzgss-body{
  padding-top: 140rpx;
}
.for-list{
  width:94%;
  background: #fff;
  margin-top: 30rpx;
  padding:15rpx 3% 15rpx 3%;
}
.for-list view:first-child{
  font-size: 30rpx;
  color:#31363B;
}
.for-list view:not(:first-child){
  font-size: 26rpx;
  color:#31363B;
  line-height: 50rpx;
}
.for-list view:last-child{
  display: flex;
}
.for-list view:last-child navigator{
  margin-left: 15rpx;
  color:#00B1E0;
  text-decoration: underline;
}





