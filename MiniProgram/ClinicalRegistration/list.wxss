/* ClinicalRegistration//list.wxss */
Page{
  margin:0;
  padding:0;
}
.select-list{
  width:94%;
  margin: auto;
  font-size: 28rpx;
  color: #626A71;
  padding-bottom: 20rpx;
}
.select-list view{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding:20rpx 0 20rpx;
}
.select-list-add{
  width:94%;
  margin: auto;
  font-size: 28rpx;
  color: #626A71;
  padding-bottom: 20rpx;
}
.data-body{
  width:100%;
  position: absolute;
  top:80rpx;
  left:0;
  z-index: 1;
}
.total{
  height:50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  color: #B6BABE;
  text-align: center;
}
.total text{
  color:#00B1E0;
}
.list-wrap{
  background: #fff;
  margin-bottom: 30rpx;
}
.list-header{
  width:94%;
  margin:auto;
  display: flex;
  justify-content: space-between;
  align-items: top;
  position: relative;
  padding:15rpx 0 15rpx;
}
.list-header view{
  width:50%;
}
.oblong{
  position: absolute;
  top:27rpx;
  left:-10rpx;
  width:6rpx;
  height:30rpx;
  background: #4A90E2;
}
.list-header view:first-child{
  font-size: 32rpx;
  color:#64666A;
  overflow: hidden;
  text-overflow: ellipsis;
  display: box;
  line-clamp: 2;
  box-orient: vertical;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.list-header view:first-child text{
  color:#36373C;
  font-family: PingFangSC-Medium;
}
.list-header view:last-child{
  color:#64666A;
}
.list-header view:last-child text{
  color:#A47951;
  font-family: PingFangSC-Medium;
}
.list-body{
  width:94%;
  margin:auto;
  font-size: 26rpx;
  color: #626A71;
}
.list-body label{
  display: block;
  padding:5rpx 0 5rpx;
}