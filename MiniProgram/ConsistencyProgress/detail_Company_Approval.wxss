/* ConsistencyProgress//detail_Company_Approval.wxss */
@import "/common/public.wxss";
Page{
  margin:0;
  padding:0;
}
.data-body{
  width:100%;
  position: absolute;
  top:80rpx;
  left:0;
  z-index: 1;
}
.total{
  height:50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  color: #B6BABE;
  text-align: center;
}
.total text{
  color:#00B1E0;
}
.list-wrap{
  background: #fff;
  padding-bottom: 15rpx;
  margin-bottom: 30rpx;
}
.list-header{
  width:94%;
  margin:auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
  padding:15rpx 0 15rpx;
}
.oblong{
  position: absolute;
  top:27rpx;
  left:-10rpx;
  width:6rpx;
  height:30rpx;
  background: #4A90E2;
}
.list-header view:first-child{
  width:70%;
  font-size: 32rpx;
  color:#64666A;
  overflow: hidden;
  text-overflow: ellipsis;
  display: box;
  line-clamp: 2;
  box-orient: vertical;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.list-header view:first-child text{
  color: #525E69;
  font-family: PingFangSC-Medium;
  font-weight: bold;
}
.list-header view:last-child{
  width:30%;
  text-align: right;
}
.list-header view:last-child text{
  border-radius: 20rpx;
  padding: 5rpx 15rpx 5rpx 15rpx;
  font-size: 22rpx;
}
.list-header view text.ispass{
  color: #009CA1;
  border:1rpx solid #009CA1;
}
.list-header view text.nopass{
  color: #B29072;
  border:1rpx solid #B29072;
}
.list-body{
  width:94%;
  margin:auto;
  font-size: 26rpx;
  color: #626A71;
}
.list-body view label{
  width:50%;
}
.list-body label{
  display: block;
  padding:5rpx 0 5rpx;
}
