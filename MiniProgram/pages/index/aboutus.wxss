
.gywm-section{
  width:90%;
  margin: auto;
}
.gywm-section view:nth-child(2){
  font-size: 26rpx;
  color:#1D1D1D;
  margin-bottom: 50rpx;
}
.gywm-section view:nth-child(3){
  font-size: 26rpx;
  color:#1D1D1D;
  padding-bottom: 50rpx;
  border-bottom:1rpx solid #DDE0E5;
}

.ptjj-title{
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 0 50rpx;
  padding-top: 50rpx;
  font-weight:bold;
}
.ptjj-title image{
  width:35rpx;
  height:35rpx;
  margin-right: 15rpx;
}

.ptjj-title text{
  color:#0F0F0F;
}

.lxwm-title{
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 50rpx 0 50rpx;
  font-weight:bold;
}
.lxwm-title image{
  width:35rpx;
  height:35rpx;
  margin-right: 15rpx;
}
.lxwm-con{
  text-align: center;
  padding:35rpx 0 35rpx 0;
}
.lxwm-con text{
  font-size: 26rpx;
  color:#1D1D1D;
}
.call-btn{
  width:500rpx;
  margin:auto;
  margin-bottom: 50rpx;
}
.call-btn button{
  font-size: 30rpx;
  color:#00BEEE;
  background: transparent;
  border:1rpx solid #00BEEE;
}


.abc{
  display: flex;
  justify-content: space-around;
  align-items: center;
  width:100%;
  height:100%;
  position: absolute;
}

.apply view{
  width:300rpx;
  height:70rpx;
  line-height:70rpx;
  margin:auto;
  margin-top:30rpx;
  text-align:center;
  background:#00BEEE;
  font-size:30rpx;
  color:#fff;
}
