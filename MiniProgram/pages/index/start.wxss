/* pages/index/start.wxss */
Page{
  background: #fff;
}
.start-title{
  width:100%;
  height:200rpx;
  line-height:200rpx;
  text-align: center;
  color:#6A6A6A;
  font-size: 35rpx;
}
.rotate-wrap{
  width:100%;
  height:200rpx;
  display: flex;
  justify-content: center;
  align-items: center; 
}
.rotate-wrap view{
  width:120rpx;
  height:120rpx;
  border-radius: 120rpx;
  overflow: hidden;
  -webkit-animation: rotate 3s linear infinite;
  animation: rotate 3s linear infinite;
}
@keyframes rotate{
  0%{
    transform: rotate(0)
  }
  100%{
    transform: rotate(360deg)
  }
}
@-webkit-keyframes rotate{
  0%{
    transform: rotate(0)
  }
  100%{
    transform: rotate(360deg)
  }
}
.rotate-wrap view image{
  width:120rpx;
  height:120rpx;
  border-radius: 120rpx;
}

.loading-img{
  width:100%;
  height:300rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.loading-img image{
  width:200rpx;
  height:150rpx;
}