/* page/pages/Dingyue/Dingyue.wxss */

.Dingyue-head{
  width:100%;
}
.head-one{
  width:100%;
  position: absolute;
  top:0;
  z-index: -1;
}
.head-one image{
  width:100%;
}
.head-two-wrap{
  display: flex;
  align-items: center;
  width:94%;
  height:215rpx;
  margin:auto;
  background: #fff;
  border-radius:10rpx;
  margin-top: 100rpx;
  position: relative;
}

.head-two{
  position: absolute;
  top:0;
  right:0;
  width:175rpx;
  height:75rpx;
}
.head-two navigator{
  width:175rpx;
  text-align:center;
  height:50rpx;
  line-height:50rpx;
  float:right;
  margin:15rpx;
  font-size: 30rpx;
  color: #FF8600;
}
.default{
  width:94%;
  margin:auto;
  display: flex;
  justify-content: start;
  height:115rpx;
}
.default-left{
  width:135rpx;
}
.default-left button{
  padding:0;
  margin:0;
  width:115rpx;
  height:115rpx;
  border-radius:115rpx;
  overflow: hidden;
}
.default-left button::after{
  border: none;
}
.default-left image{
  width:115rpx;
  height:115rpx;
  border-radius: 115rpx;
  display: block;
  overflow: hidden;
}
.right-one{
  height:60rpx;
  line-height: 60rpx;
  display: flex;
  justify-content: start;
}
.right-one button{
  height:35rpx;
  line-height: 35rpx;
  margin-top:10rpx;
  margin-bottom: 15rpx;
  padding:0;
  background: transparent;
  color: #000000;
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
}
.right-one button::after{
  border: none;
}

.right-two{
  height:45rpx;
  display: flex;
  justify-content: start;
  align-items: center;
}
.free{
  margin-right: 15rpx;
  text-align: center;
  color: #3FC1A3;
  font-size: 22rpx;
  height:45rpx;
  line-height:45rpx;

}
.binding{
  background:#00BEEE;
  color:#fff;
  padding:5rpx 15rpx 5rpx 15rpx;
  border-radius: 50rpx;
  font-size:22rpx;
  height:35rpx;
  line-height:35rpx;

}
.vip{
  background: #323232;
  color: #FFD07B;
  padding:3rpx 5rpx 3rpx 5rpx;
  margin-right: 15rpx;
  font-size: 22rpx;
  height:34rpx;
  line-height: 34rpx;
}

.vip image{
  width:20rpx;
  height:20rpx;
  margin:0 3rpx -1rpx 3rpx;
}

.untying{
  color: #81878C;
  font-size: 22rpx;
}
.careful{
  width:100%;
  height:50rpx;
  line-height: 50rpx;
  text-align: center;
  position: absolute;
  bottom:5rpx;
  left:0;

}
.careful text{
  font-size: 24rpx;
  color: #DF592E;
}


.Dingyue-body{
  background: #fff;
  margin-top: 35rpx;
}

.for-con{
  width:94%;
  height:160rpx;
  margin:auto;
  display: flex;
  justify-content: space-between;
  align-items:center;
}
.for-left{
  width:75%;
  display: flex;
  justify-content: flex-start;
}
.left-left{
  width:25%;
  text-align: center;
  padding-top: 15rpx;
}
.left-right{
  width:75%;
}
.for-center{
  width:5%;
  text-align: right;
}
.for-center text{
  color:#F1F3F5;
  background: #F1F3F5;
  padding:15rpx 0 15rpx;
}


.left-left image{
  width:75rpx;
  height:74rpx;
}
.left-right view:nth-child(1){
  font-size: 30rpx;
  color: #31363B;
  font-family:PingFangSC-Medium;
  font-weight:bold;
}
.left-right view:nth-child(2){
  font-size: 24rpx;
  color: #81878C;
}
.for-right{
  width:20%;
}
.for-right view view{
  text-align: center;
  font-size: 34rpx;
  color: #00BEEE;
}
.for-right view view.grey{
  color: #7A7A7A;
}
.for-right view view.hope{
  font-size: 30rpx;
  color: #7A7A7A;
}

.QR-code{
  width:100%;
  margin:auto;
  padding: 30rpx 0 30rpx;
  background-color: #F6F6F7;
}
.part1{
  width:100%;
  height:50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.part1 image{
  width:40rpx;
  height:21rpx;
}
.part1 text{
  margin:0 15rpx 0 15rpx;
  font-size: 32rpx;
  color: #2A2A2A;
}
.part2{
  text-align: center;
  font-size: 22rpx;
  color: #81878C;
}
.part3{
  width:94%;
  height:150rpx;
  margin:auto;
  display: flex;
  justify-content: center;
  align-items: center;
  border-bottom: 1rpx solid #DBDBDB;
}
.part3 view{
  margin-left: 10rpx;
  background: #00BEEE;
  color:#fff;
  font-size: 28rpx;
  padding:5rpx 15rpx 5rpx 15rpx;
}
.part4{
  width:94%;
  height:200rpx;
  margin:auto;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top:30rpx;
}
.part4 image{
  width:200rpx;
  height:200rpx;
  margin-right: 10rpx;
}
.part3 text,.part4 text{
  font-size: 28rpx;
  color: #81878C;
}
