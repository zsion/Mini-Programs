.prompt{
  position: absolute;
  top:0;
  right:0;
  z-index: 3;
  width:71.5%;
  max-height:500rpx;
  background: #fff;
  border: 1px solid #D7DADF;
  box-shadow: -3px -2px 7px 0 rgba(0,0,0,0.09);
  font-size: 26rpx;
  color: #5C6376;
}
.prompt view{
  width:97%!important;
  padding:15rpx 5rpx 15rpx 5rpx;
}
.prompt-other{
  position: absolute;
  right:0;
  left:0;
  z-index: 2;
  width:94%!important;
  margin:auto;
  background: rgba(255,255,255,0.7);
}
.close{
  width:45rpx;
  height:45rpx;
  border-radius: 45rpx;
  position:absolute;
  top:1rpx;
  right:1rpx;
  z-index:5;
  display: flex;
  align-items: top;
}
.close image{
  width:100%;
  border-radius: 45rpx;
}

