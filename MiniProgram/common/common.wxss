.body-wrap{
  width:94%;
  margin:auto;
}
.list-wrap{
  position: relative;
  background: #fff;
  padding: 15rpx;
  margin:20rpx 0 20rpx;
}
.list-tip{
  width:80rpx;
  height:45rpx;
  line-height: 45rpx;
  text-align: center;
  position: absolute;
  top:0;
  right:0;
  font-size: 22rpx;
  color:#fff;
}
.color_bg0{
  background: #FEB881;
}
.color_bg1{
  background: #55D1D4;
}
.color_bg2{
  background: #79B7FF;
}
.list-title{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 26rpx;
  color: #31363B;
  line-height: 70rpx;
}
.list-title image{
  width:25rpx;
  height:25rpx;
  margin-right: 5rpx;
}
.list-content{
  font-size: 26rpx;
  color: #909599;
  padding-bottom: 15rpx;
  border-bottom: 1rpx solid #ECEEF2;
}
.list-wrap view:last-child{
  border: none;
}
